package com.app;

import org.springframework.beans.factory.annotation.Value;

public class DataBaseConfg1 {
	
	@Value("${my.db.driver}")
	private String driver;
	
	@Value("${my.db.url}")
	private String url;
	
	@Value("${my.db.usr}")
	private String user;
	
	@Value("${my.db.pwd}")
	private String password;
	
	
	@Override
	public String toString() {
		return "DataBaseConfg1 [driver=" + driver + ", url=" + url + ", user=" + user + ", password=" + password + "]";
	}
	
	
}
