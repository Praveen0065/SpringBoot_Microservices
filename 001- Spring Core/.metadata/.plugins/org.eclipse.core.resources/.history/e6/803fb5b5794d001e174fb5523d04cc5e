package com.app;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.stereotype.Component;

@Component
public class Process1 {
	
	//@Value("#{ 2 + 3 }")
	@Value("#{ 'Hello ABC'.length()} ")
	private int code;

	@Override
	public String toString() {
		return "Process1 [code=" + code + "]";
	}
	
	

}
